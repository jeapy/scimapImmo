
	{% extends "admin/layout.html.twig" %}

	 {% block title %}Souscripteur{% endblock %}



			     {% block content %}

  			
  			<div class="content-box-large">
  				<div class="panel-heading">
					<div class="panel-title">	<h1>Listes des souscripteurs	</h1></div>
				</div>
  				<div class="panel-body">
  					<div class="table-responsive">
  						<table class="table">
			              <thead>
			                <tr>
			                  <th>#</th>
			                  <th>First Name</th>
			                  <th>Last Name</th>
			                  <th>Subscribe Date</th>
			                  <th>Subscribe Plan</th>
			                </tr>
			              </thead>

			               {% for artist in artists %}

			               <tbody>
			                <tr>
			                  <td>{{ artist.souscrire.id }}</td>
			                  <td>{{ artist.souscrire.firstname|e }}</td>
			                  <td>{{ artist.souscrire.lastname|e }}</td>
			                  <td>{{ artist.souscrire.createAt | date('d-m-Y H:i') }}</td>
			                 
			                  
			                  {% if artist.plan == 1 %}
			                   <td>PLAN PERSONNEL</td>
			                  {% else %}
			                   <td>PLAN SCIMAF</td>
			                   {% endif %}

			                   <td> <a class="btn btn-primary" href="{{ path('souscripteur_show', {'subscribe': artist.id}) }}">Details</a></td>
			                </tr>
			                 </tbody>
                    
                {% endfor %}
			              
			             
			            </table>
  					</div>
  				</div>
  				{% include "admin/pagination.html.twig" %}
  			</div>

 </div>
  			

{% endblock %}


